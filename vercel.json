{
    "version": 2,
    "builds": [
        {
            "src" : "wiki/wsgi. py",
            "use": "@vercel/python",
            "config": { "maxLambdaSize": "15mb", "runtime": "python3.10.5" }
        },
        {
            "src": "build files.sh",
            "use" : "@vercel/static-build",
            "config": {
                "distDir": "staticfiles build"
            }
        }
    ],
    " routes " : [ 
        {
            "src" : "/static/(.*)",
            "dest": "/static/$l"
        },
        {
            "src": "/(.*)",
            "dest": "poll/wsgi. py"
        }
    ]
}
